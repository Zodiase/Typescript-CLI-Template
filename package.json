{
  "name": "tscli-template-generator",
  "version": "1.0.0",
  "description": "Tool for generating templates for CLI tools written in TypeScript.",
  "engines": {
    "node": ">=16"
  },
  "bin": {
    "tscli-create": "bin.cjs"
  },
  "main": "dist/main.js",
  "scripts": {
    "dev:run": "node bin.cjs",
    "dist:run": "node bin.cjs",
    "dist:build": "npx tsc --build src",
    "dist:clean": "rm -rf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Zodiase/Typescript-CLI-Template.git"
  },
  "author": "Xingchen Hong",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Zodiase/Typescript-CLI-Template/issues"
  },
  "homepage": "https://github.com/Zodiase/Typescript-CLI-Template#readme",
  "devDependencies": {
    "@types/node": "^20.3.1",
    "@types/semver": "^7.5.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "semver": "^7.5.2"
  }
}
